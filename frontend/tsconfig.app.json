{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "bindings/urodstvo-launcher/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowJs": true,
    "paths": {
      "@/*": ["./src/*"],
      "@go": ["./bindings/urodstvo-launcher"],
      "@go/*": ["./bindings/urodstvo-launcher/*"]
    }
  }
}
